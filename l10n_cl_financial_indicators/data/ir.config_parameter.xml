<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record model='ir.config_parameter' id='sbif_fin_url'>
            <field name='key'>sbif.financial.indicators.baseurl</field>
            <field name='value'>http://api.sbif.cl/api-sbifv3/recursos_api</field>
        </record>
        <!-- this can be removed in order to be asked for token in cl_base -->
        <record model='ir.config_parameter' id='sbif_fin_apikey'>
            <field name='key'>sbif.financial.indicators.apikey</field>
            <field name='value'>PUT_HERE_YOUR_SBIF_TOKEN</field>
        </record>
    </data>
</odoo>
